{
  "name": "@belsrc/vue-gen",
  "version": "0.2.1",
  "description": "Yet another Vue component generator",
  "main": "vue-gen.js",
  "bin": {
    "vuegen": "vue-gen.js"
  },
  "gitHooks": {
    "commit-msg": "commitlint -E GIT_PARAMS",
    "pre-commit": "npm run precommithook"
  },
  "scripts": {
    "precommithook": "npm-run-all clean test:changed-silent",
    "check": "npx npm-check -u",
    "lint": "npx eslint -c .eslintrc.js --fix --color --ignore-path .eslintignore \"src/**/*.js\"",
    "prettier": "prettier --config .prettierrc.js --write \"src/**/*.js\"",
    "clean": "npm-run-all prettier lint",
    "test": "jest --verbose --passWithNoTests",
    "test:changed": "jest -o --passWithNoTests",
    "test:changed-silent": "jest -o --silent --passWithNoTests",
    "test:cov": "jest --verbose --coverage --passWithNoTests",
    "coverage": "jest --verbose --coverage --passWithNoTests && codecov -t 12ffff01-901f-4bec-8cf2-e3c7f4b5d313",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "standard": "standard-version --no-verify",
    "push-tags": "git push --follow-tags origin master",
    "pub": "npm publish",
    "release": "npm-run-all clean test standard push-tags pub"
  },
  "jest": {
    "clearMocks": true,
    "resetModules": true,
    "coverageDirectory": "coverage",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "vue component generator",
    "vue generator",
    "vue component",
    "vue cli",
    "vue",
    "component",
    "generator",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/belsrc/vue-gen.git"
  },
  "bugs": {
    "url": "https://github.com/belsrc/vue-gen/issues"
  },
  "homepage": "https://github.com/belsrc/vue-gen#readme",
  "author": "bryan@bryanckizer.com",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "8.2.0",
    "@commitlint/config-conventional": "8.2.0",
    "codecov": "3.6.1",
    "conventional-changelog-cli": "2.0.23",
    "eslint": "6.5.0",
    "eslint-config-belsrc": "1.1.0",
    "jest": "24.9.0",
    "npm-check": "5.9.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "standard-version": "7.0.0",
    "yorkie": "2.0.0"
  },
  "dependencies": {
    "commander": "3.0.2",
    "fjp": "0.2.4",
    "inquirer": "7.0.0"
  }
}
